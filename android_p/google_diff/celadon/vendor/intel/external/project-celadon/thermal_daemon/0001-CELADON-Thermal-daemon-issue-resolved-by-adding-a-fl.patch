From 19841bb50b432a59ff01cd1aad98f65d349787a2 Mon Sep 17 00:00:00 2001
From: swaroopb <swaroop.balan@intel.com>
Date: Wed, 12 Dec 2018 14:28:00 +0530
Subject: [PATCH] [CELADON] Thermal daemon issue resolved  by adding a flag

Tracked-On: None
Signed-off-by: swaroopb <swaroop.balan@intel.com>
---
 Android.mk | 6 +++++-
 1 file changed, 5 insertions(+), 1 deletion(-)

diff --git a/Android.mk b/Android.mk
index ae68067..b1f5354 100644
--- a/Android.mk
+++ b/Android.mk
@@ -51,7 +51,11 @@ LOCAL_CFLAGS := \
 		-Werror
 
 LOCAL_STATIC_LIBRARIES := libxml2
-LOCAL_SHARED_LIBRARIES := liblog libcutils libdl libc++ libicuuc libutils
+ifeq ($(BOARD_VNDK_VERSION),current)
+       LOCAL_SHARED_LIBRARIES := liblog libcutils libdl libc++ libutils
+else
+       LOCAL_SHARED_LIBRARIES := liblog libcutils libdl libc++ libicuuc libutils
+endif
 LOCAL_PRELINK_MODULE := false
 LOCAL_MODULE := thermal-daemon
 include $(BUILD_EXECUTABLE)
